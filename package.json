{
  "name": "sphere-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/config/vue/vue.config.js vue-cli-service serve --mode development",
    "serve-down": "./scripts/maintenance/down.sh && npm run serve",
    "serve-up": "./scripts/maintenance/up.sh && npm run serve",
    "build": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/config/vue/vue.config.js vue-cli-service build --mode production && ./scripts/uglify.sh $PWD",
    "build-patch": "npm version patch && npm run build",
    "build-minor": "npm version minor && npm run build",
    "build-major": "npm version major && npm run build",
    "build-down": "./scripts/maintenance/down.sh && npm run project-install --down",
    "build-up": "./scripts/maintenance/up.sh && npm run project-install --up",
    "project-install": "./scripts/deploy.sh",
    "project-update": "./scripts/update.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@sofiakb/axios-api": "1.0.0-beta.1",
    "@sofiakb/cookie": "^1.0.0-beta.3",
    "axios": "^0.27.2",
    "core-js": "^3.22.4",
    "jquery": "^3.6.0",
    "json-cookie": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "pluralize": "^8.0.0",
    "register-service-worker": "^1.7.2",
    "sweetalert2": "^11.4.10",
    "vue": "^3.2.33",
    "vue-final-modal": "^3.4.3",
    "vue-meta": "^3.0.0-alpha.8",
    "vue-router": "^4.0.15",
    "vue-routisan": "^3.0.0-beta.5",
    "vue-web-storage": "^6.1.0",
    "vue3-perfect-scrollbar": "^1.6.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-plugin-vuex": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.33",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "typescript": "~4.6.4"
  }
}
